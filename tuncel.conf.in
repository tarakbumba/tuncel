# rpm2cpio Binary Name
RPM2CPIO_BIN=@RPM2CPIO_BIN@

# rpm Binary Name
RPM_BIN=@RPM_BIN@

# cpio Binary Name
CPIO_BIN=@CPIO_BIN@

# md5sum Binary Name
MD5SUM_BIN=@MD5SUM_BIN@

# kdialog Binary Name
KDIALOG_BIN=@KDIALOG_BIN@

# zenity binary;
ZENITY_BIN=@ZENITY_BIN@

# Default Terminal Command
# Usage: TERMINAL_CMD TERMINAL_OPTS
## @example: "konsole --workdir"
# Use terminal options here
TERMINAL_APPLICATION="@TERMINAL@"
TERMINAL_OPTS=" "

# Temp Directory Default /tmp
TMP_DIR=/tmp

