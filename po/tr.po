msgid "Error: Missing tuncel configuration file"
msgstr "Hata: tuncel yapılandırma dosyası bulunamadı"

#: ../tuncel.in:51
msgid "Executable file can not be found: "
msgstr "Yürütülebilir dosya bulunamıyor: "

#: ../tuncel.in:100
msgid ""
"tuncel commands:\n"
"maininfo :  Display Package Main Info\n"
"showcontents : Show Package Contents in Info Screen\n"
"unpack : Unpack package in current Directory\n"
"unpackto : Unpack Package to Directory\n"
"openHomepage : If %{url} exists open Package Homepage in Web Browser\n"
"testinstall : Installation Test (-iv)\n"
"forceinstalltest : Force Installation Test (-Uhv)\n"
"installsrc : Install RPM Source Package in to Build Enviroment\n"
"Example: tuncel maininfo /tmp/foo-0.0.5.i586.rpm\n"
msgstr ""
"tuncel komutları:\n"
"maininfo :  Paket ana bilgilerini görüntüle\n"
"showcontents : Paket içeriği bilgi ekranında göster\n"
"unpack : Paketi geçerli dizine aç\n"
"unpackto : Paketi bir dizine aç\n"
"openHomepage : %{url} mevcutsa paket ana sayfasını web tarayıcıda aç\n"
"testinstall : Kurulum sınaması (-iv)\n"
"forceinstalltest : Zorlanmış Kurulum Sınaması (-Uhv)\n"
"installsrc : RPM kaynak paketini derleme ortamına kur\n"
"Örnek: tuncel maininfo /tmp/foo-0.0.5.i586.rpm\n"

